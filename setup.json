{
    "name": "aiida_mul",
    "author": "Rico Haeuselmann",
    "author_email": "haeuselm@epfl.ch",
    "description": "Minimal AiiDA plugin for testing and getting started with development",
    "url": "https://github.com/DropD/aiida-mul",
    "license": "Creative Commons",
    "classifiers": [
        "Programming Language :: Python"
    ],
    "version": "0.1",
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida >= 0.8"
    ],
    "entry_points": {
        "console_scripts": [
            "mul = aiida_mul.code:multiply"
        ],
        "aiida.data": [
            "mul.parameters = aiida_mul.data:MultiplyParameters"
        ],
        "aiida.calculations": [
            "mul.multiply = aiida_mul.calcs:MultiplyCalculation"
        ],
        "aiida.parsers": [
            "mul.product = aiida_mul.parsers:MultiplyParser"
        ]
    },
    "scripts": ["examples/cli.py"]
}
